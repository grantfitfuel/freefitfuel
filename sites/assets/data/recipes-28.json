[
  {
    "title": "Spiced Chickpea & Roast Carrot Traybake with Yoghurt (Foodie Fuel Budget 1)",
    "mealType": "Dinner",
    "serves": 2,
    "time_mins": 40,
    "cook_temp_c": 210,
    "ingredients": [
      {
        "item": "Carrots, cut into batons",
        "qty": "400 g"
      },
      {
        "item": "Tinned chickpeas, drained",
        "qty": "1 x 400 g tin"
      },
      {
        "item": "Ground cumin",
        "qty": "1 tsp"
      },
      {
        "item": "Ground coriander",
        "qty": "1 tsp"
      },
      {
        "item": "Smoked paprika",
        "qty": "1 tsp"
      },
      {
        "item": "Olive oil",
        "qty": "1½ tbsp"
      },
      {
        "item": "Greek-style yoghurt",
        "qty": "120 g"
      },
      {
        "item": "Lemon juice",
        "qty": "1 tbsp"
      }
    ],
    "method": [
      "Preheat oven to 210°C (fan 190°C).",
      "Toss carrots and chickpeas with spices, oil and seasoning.",
      "Roast for 35–40 minutes, turning once, until caramelised.",
      "Mix yoghurt with lemon juice and a pinch of salt.",
      "Serve roasted vegetables with yoghurt spooned over."
    ],
    "optional_finishes": [
      "Pickled carrot ribbons",
      "Chilli oil drizzle"
    ],
    "macros": {
      "kcal": 460,
      "protein_g": 21,
      "carbs_g": 44,
      "fat_g": 18,
      "fibre_g": 14
    },
    "allergens": [
      "Milk"
    ]
  },
  {
    "title": "Harissa Chicken Thighs with Lemon Couscous (Foodie Fuel Budget 2)",
    "mealType": "Dinner",
    "serves": 2,
    "time_mins": 35,
    "cook_temp_c": 200,
    "ingredients": [
      {
        "item": "Chicken thighs, skinless",
        "qty": "4 (approx. 500 g)"
      },
      {
        "item": "Harissa paste",
        "qty": "2 tbsp"
      },
      {
        "item": "Olive oil",
        "qty": "1 tbsp"
      },
      {
        "item": "Red onion, sliced",
        "qty": "1"
      },
      {
        "item": "Wholewheat couscous",
        "qty": "120 g"
      },
      {
        "item": "Vegetable stock, boiling",
        "qty": "200 ml"
      },
      {
        "item": "Lemon, zest and juice",
        "qty": "1"
      }
    ],
    "method": [
      "Preheat the oven to 200°C (fan 180°C).",
      "Rub the chicken thighs with harissa, olive oil and seasoning.",
      "Place chicken and red onion on a tray and roast for 30–35 minutes until cooked through.",
      "Cover couscous with boiling stock, cover and steam for 5 minutes.",
      "Fluff couscous, stir through lemon zest and juice, and serve with the chicken."
    ],
    "optional_finishes": [
      "Quick-pickled red onion",
      "Preserved lemon, finely chopped"
    ],
    "macros": {
      "kcal": 520,
      "protein_g": 42,
      "carbs_g": 38,
      "fat_g": 18,
      "fibre_g": 11
    },
    "allergens": [
      "Gluten"
    ]
  },
